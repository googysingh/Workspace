package xtraprograms;

public class Singleton {

	private static Singleton obj=null;
	
	private Singleton() {
		
	}
	
	public  static Singleton init(){
		if(obj==null) {
			try {
				Thread.sleep(300);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			obj = new Singleton();
		}
		return obj;
	}

}
